print "\nwhile loop\n";

var i = 0;
while (i < 3) {
    var j = 0;
    while (j < 3) {
        print "" + i + ", " + j + "\n";
        j = j + 1;
    }
    i = i + 1;
}

print "\nfor loop\n";
for (var x = 0; x < 3; x = x + 1)
    for (var y = 0; y < 3; y = y + 1)
        print "" + x + ", " + y + "\n";